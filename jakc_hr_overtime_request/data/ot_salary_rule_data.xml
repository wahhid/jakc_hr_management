<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--Salary rule for overtime in hours-->
        <record id="hr_salary_rule_ot_hours" model="hr.salary.rule">
            <field name="code">OT(hous)</field>
            <field name="name">Overtime(hours)</field>
            <field name="category_id" ref="hr_payroll.ALW"/>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = employee.get_overtime_hours(employee.id, payslip.date_from, payslip.date_to)</field>
            <field name="sequence" eval="11"/>
        </record>
    </data>
</openerp>